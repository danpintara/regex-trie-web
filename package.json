    "@types/enzyme": "^3.10.5",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-measure": "^2.0.6",
    "@types/react-transition-group": "^4.4.0",
    "@types/sinon": "^9.0.4",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.0.1",
    "enzyme": "^3.11.0",
